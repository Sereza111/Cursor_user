{
  "name": "cline-local-rotator",
  "version": "3.0.0",
  "description": "Локальный скрипт для ротации API KEY CLINE в VS Code. Получает API KEY с сервера и устанавливает его напрямую в VS Code через keytar.",
  "main": "rotator.js",
  "scripts": {
    "start": "node rotator.js",
    "check": "node rotator.js --check-only",
    "test-fetch": "node rotator.js --test-fetch",
    "force": "node rotator.js --force",
    "daemon": "node daemon.js",
    "help": "node rotator.js --help"
  },
  "dependencies": {
    "keytar": "^7.9.0",
    "node-fetch": "^2.7.0",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
